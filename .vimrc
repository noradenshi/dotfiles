let mapleader = " "

"set bg=dark
set hlsearch
set incsearch
set clipboard=unnamedplus

set tabstop=4
set softtabstop=4
set shiftwidth=4
set autoindent
set fileformat=unix

set nocompatible
syntax on
set termguicolors
colorscheme catppuccin_mocha

set encoding=utf-8
set number relativenumber
set wildmode=longest,list,full

set splitbelow splitright

noremap <leader>pv <cmd>Ex<CR>
noremap <leader>no <cmd>noh<CR>
